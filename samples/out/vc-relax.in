	diagonalization='david',
	mixing_mode = 'plain',
	mixing_beta = 0.7,
	conv_thr =  1e-6,
	electron_maxstep = 50
	scf_must_converge = .true.
/ 

&ions
	ion_dynamics = 'bfgs'
/

&cell
	cell_dynamics='bfgs'
	cell_factor = 5
/

