	calculation='vc-relax',
	pseudo_dir= './ps/'
	prefix = 'QE'
	outdir= './out/'
	verbosity = 'low'
	nstep = 100
	restart_mode = 'restart'
	etot_conv_thr = 1e-6,
	forc_conv_thr = 1e-3,
/ 

&system
	ibrav= 0,
	nat= 100, 
	ntyp= 2,
	ecutwfc= 30,
	input_dft= 'PBE',
	occupations= 'smearing',
	smearing= 'gaussian',
	degauss= 0.0012,
/

